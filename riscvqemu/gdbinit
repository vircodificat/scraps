target remote :1234
set architecture riscv:rv64

add-symbol-file \
    build/vmlinux 0x80202000 \
    -s .head.text 0x80200000 \
    -s .init.text 0x802e1940 \
    -s .init.data 0x802ff000 \
    -s .init.pi   0x80309e98 \
    -s .init.bss  0x8030c788

symbol-file build/vmlinux -o 0x100200000
file build/vmlinux

break panic
